{
  "expo": {
    "name": "Otona St.Mary Library Management App",
    "displayName": "Otona St.Mary LibMgt",
    "slug": "church-library-app",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/icons/android/mipmap-mdpi/church_library_app.png",
    "scheme": "churchlibraryapp",
    "router": {
      "origin": false
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.churchlibraryapp",
      "icon": "./assets/icons/ios/AppIcon.appiconset/Icon-App-60x60@2x.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android/mipmap-mdpi/church_library_app_foreground.png",
        "backgroundImage": "./assets/icons/android/values/ic_launcher_background.xml",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/icons/android/mipmap-mdpi/church_library_app.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_MEDIA_LOCATION",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.churchlibraryapp",
      "jsEngine": "hermes"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow this app to access your photos",
          "savePhotosPermission": "Allow this app to save photos",
          "isAccessMediaLocationEnabled": true,
          "audioPermission": "Allow this app to access your audio files"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "148c26d2-cd12-414f-ae4a-ae98d7d4fa1e"
      }
    }
  }
}