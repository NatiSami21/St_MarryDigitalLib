{
  "expo": {
    "name": "FAYDA Smart Library Management",
    "slug": "church-library-app",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "churchlibraryapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,


    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.diguwasoft.faydasmartlibrary",
      "jsEngine": "hermes",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,

      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },

      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "READ_MEDIA_AUDIO",
        "ACCESS_MEDIA_LOCATION"
      ]
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash.png",
          "resizeMode": "cover",
          "ios": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "enableFullScreenImage_legacy": true
          },
          "android": {
            "image": "./assets/icon.png",
            "imageWidth": 200,
            "backgroundColor": "#ffffff"
          }
        }
      ],
      "expo-router",
      "expo-media-library",
      "expo-sqlite",
      "expo-secure-store",
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "eas": {
        "projectId": "148c26d2-cd12-414f-ae4a-ae98d7d4fa1e"
      }
    }
  }
}